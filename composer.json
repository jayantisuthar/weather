{
    "name": "dashcode/weather",
    "description": "AccuWeather API Data",
    "license": "Apache-2.0",
    "version" : "1.0.0",
    "type": "library",
    "keywords": [
        "accu weather",
        "weather",
        "accu",
        "api"
    ],
    "authors": [
        {
            "name": "jayanti suthar",
            "email": "suthar.jayanti93@gmail.com",
            "homepage": "https://jayantisuthar.github.io"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "7.9.x-dev",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "DashCode\\": "src/Weather/"
        }
    },
    "scripts": {
